      },
      "source": [
        "#For Arrival Time\n",
        "a_time=[]\n",
        "j=0\n",
        "for i in range(0,len(inter_a_time)):\n",
        "    j+=inter_a_time[i]\n",
        "    a_time.append(j)\n",
        "     \n",
        "print(a_time)"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
