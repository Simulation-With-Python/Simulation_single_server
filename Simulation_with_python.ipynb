        "id": "cFbsgHDT3EsR"
      },
      "source": [
        "#For Service time begin\n",
        "service_time_begin = []\n",
        "for i in range(0,20):\n",
        "  if a_time[i]> time_s_end[i-1]:\n",
        "    service= a_time[i]\n",
        "  else: service= time_s_end[i-1]\n",
        "  service_time_begin.append(service)\n",
        "             \n",
        "\n"
      ],
